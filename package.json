{
	"name": "auth-service",
	"version": "1.0.0",
	"description": "a simple auth-service made by node.js, express.js, postgres, prisma",
	"main": "./dist/index.js",
	"type": "module",
	"scripts": {
		"build": "tsc",
		"start": "cross-env NODE_ENV=production node dist/index.js",
		"postbuild": "node ./scripts/js-extensions.scripts.js",
		"watch": "cross-env NODE_ENV=development tsc --watch",
		"dev": "cross-env NODE_ENV=development concurrently \"npm run watch\" \"nodemon dist/index.js\" --delay 1",
		"prisma:studio": "dotenv -e .env.development -- npx prisma studio",
		"prisma:generate": "dotenv -e .env.development -- npx prisma generate",
		"prisma:migrate": "dotenv -e .env.development -- npx prisma migrate dev "
	},
	"author": "jayash saini",
	"license": "ISC",
	"devDependencies": {
		"@types/bcryptjs": "^2.4.6",
		"@types/compression": "^1.7.5",
		"@types/cookie-parser": "^1.4.8",
		"@types/cors": "^2.8.17",
		"@types/express": "^5.0.0",
		"@types/jsonwebtoken": "^9.0.7",
		"@types/morgan": "^1.9.9",
		"@types/multer": "^1.4.12",
		"@types/node": "^22.10.5",
		"@types/request-ip": "^0.0.41",
		"@types/swagger-jsdoc": "^6.0.4",
		"@types/swagger-ui-express": "^4.1.7",
		"cross-env": "^7.0.3",
		"dotenv-cli": "^8.0.0",
		"prisma": "^6.2.1",
		"ts-node": "^10.9.2",
		"tsconfig-paths": "^4.2.0",
		"tsx": "^4.19.2",
		"typescript": "^5.7.3"
	},
	"dependencies": {
		"@aws-sdk/client-s3": "^3.774.0",
		"@aws-sdk/s3-request-presigner": "^3.774.0",
		"@prisma/client": "^6.2.1",
		"bcryptjs": "^2.4.3",
		"compression": "^1.8.0",
		"concurrently": "^9.1.2",
		"cookie-parser": "^1.4.7",
		"cors": "^2.8.5",
		"dotenv": "^16.4.7",
		"express": "^4.21.2",
		"express-rate-limit": "^7.5.0",
		"ioredis": "^5.6.0",
		"jsonwebtoken": "^9.0.2",
		"kafkajs": "^2.2.4",
		"mailgun.js": "^11.1.0",
		"morgan": "^1.10.0",
		"multer": "^1.4.5-lts.2",
		"nodemon": "^3.1.9",
		"request-ip": "^3.3.0",
		"ts-node-dev": "^2.0.0",
		"ua-parser-js": "^2.0.0",
		"uuid": "^11.0.5",
		"winston": "^3.17.0",
		"zod": "^3.24.1"
	}
}
